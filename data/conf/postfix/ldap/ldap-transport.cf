server_host = localhost
server_port = 389
version = 3
bind = yes
start_tls = no
bind_dn = cn=vmail,o=openemail,{{ MAIL_LDAP_BASE_DN }}
bind_pw = {{ MAIL_LDAP_BIND_PW }} 
search_base = o=openemail,{{ MAIL_LDAP_BASE_DN }}
scope = sub
query_filter = (&(&(vd=%s)(objectClass=VirtualDomain))(accountActive=TRUE)(delete=FALSE)) 
result_attribute = postfixTransport 
